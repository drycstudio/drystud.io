{
	"name": "@euclidesdry/electron-pretty-titlebar",
	"version": "0.3.2",
	"type": "module",
	"main": "./dist/main.cjs",
	"module": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.cjs"
		},
		"./config": {
			"import": "./dist/config.js",
			"require": "./dist/config.cjs"
		}
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"license": "MIT",
	"description": "A pretty way to add Titlebar in a Electron app using ReactJS",
	"keywords": [
		"awesome",
		"pretty",
		"electron",
		"react",
		"titlebar",
		"menu",
		"plugin"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/euclidesdry"
	},
	"repository": "https://www.github.com/euclidesdry/electron-pretty-titlebar",
	"author": "euclidesdry <euclidesdry@gmail.com>",
	"packageManager": "yarn@4.3.1",
	"scripts": {
		"ci": "yarn install && yarn lint && yarn test",
		"dev": "vite",
		"build": "tsc --p ./tsconfig.build.json && vite build",
		"preview": "tsc --p ./tsconfig.build.json && vite preview",
		"build:with-storybook": "yarn build && yarn build-storybook",
		"publish:package": "yarn build && npm publish --access public",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
		"lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
		"lint:format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build --docs",
		"storybook:docs": "storybook dev --docs",
		"build-storybook": "storybook build",
		"prepare": "husky install",
		"prepublishOnly": "npm run build"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "yarn lint",
		"*.{js,jsx,ts,tsx,css,md,html,json}": "yarn lint:format"
	},
	"sideEffects": [
		"**/*.css"
	],
	"peerDependencies": {
		"electron": "*",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.2.1"
	},
	"dependencies": {
		"@stitches/core": "^1.2.8",
		"@stitches/react": "^1.2.8",
		"is-electron": "^2.2.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.6.1",
		"@eslint/compat": "^1.1.0",
		"@eslint/js": "^9.6.0",
		"@storybook/addon-actions": "^8.1.11",
		"@storybook/addon-essentials": "^8.1.11",
		"@storybook/addon-interactions": "^8.1.11",
		"@storybook/addon-links": "^8.1.11",
		"@storybook/addon-onboarding": "^8.1.11",
		"@storybook/blocks": "^8.1.11",
		"@storybook/react": "^8.1.11",
		"@storybook/react-vite": "^8.1.11",
		"@storybook/test": "^8.1.11",
		"@testing-library/dom": "^10.3.1",
		"@testing-library/react": "^16.0.0",
		"@types/node": "^20.14.10",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/react-test-renderer": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitest/coverage-v8": "^1.6.0",
		"c8": "^10.1.2",
		"electron": "^31.1.0",
		"eslint": "9.x",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-config-universe": "^13.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.9.0",
		"eslint-plugin-n": "^17.9.0",
		"eslint-plugin-promise": "^6.4.0",
		"eslint-plugin-react": "^7.34.3",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-styled-components-a11y": "^2.1.34",
		"globals": "^15.8.0",
		"husky": "^9.0.11",
		"identity-obj-proxy": "^3.0.0",
		"jsdom": "^24.1.0",
		"lint-staged": "^15.2.7",
		"prettier": "3.3.2",
		"react-icons": "^5.2.1",
		"react-test-renderer": "^18.3.1",
		"sass": "^1.77.6",
		"storybook": "^8.1.11",
		"turbo": "^2.0.6",
		"typescript": "^4.9.4",
		"typescript-eslint": "^7.15.0",
		"vite": "^5.3.3",
		"vite-plugin-dts": "^3.9.1",
		"vite-plugin-lib-inject-css": "^2.1.1",
		"vite-plugin-svgr": "^4.2.0",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.6.0"
	}
}
