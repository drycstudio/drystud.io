{
	"name": "@euclidesdry/electron-pretty-titlebar",
	"license": "MIT",
	"version": "0.0.4-beta-6",
	"description": "A pretty way to add Titlebar in a Electron app using ReactJS",
	"keywords": [
		"awesome",
		"pretty",
		"electron",
		"react",
		"titlebar"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/euclidesdry"
	},
	"repository": "https://www.github.com/euclidesdry/electron-pretty-titlebar",
	"author": "euclidesdry <euclidesdry@gmail.com>",
	"private": false,
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"files": [
		"dist"
	],
	"types": "dist/index.d.ts",
	"scripts": {
		"ci": "npm install && npm run lint && npm run test && npm run build",
		"build": "npm run rollup",
		"build:with-storybook": "npm run rollup && npm run build-storybook",
		"publish:package": "npm run build && npm run publish --access public",
		"tailwind": "npx tailwindcss -i ./src/index.scss -o ./src/components/Titlebar/titlebar.module.css",
		"tailwind:minify": "npx tailwindcss -i ./src/index.scss -o ./src/components/Titlebar/titlebar.module.css --minify",
		"tailwind:watch": "npx tailwindcss -i ./src/index.scss -o ./src/components/Titlebar/titlebar.module.css --watch",
		"rollup": "rollup -c",
		"test": "jest --runInBand --logHeapUsage",
		"test:silent": "jest --silent",
		"test:update": "jest --verbose --coverage --updateSnapshot",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
		"lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
		"lint:format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
		"storybook": "start-storybook -p 6086",
		"build-storybook": "npm run tailwind:minify && build-storybook",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "npm run lint",
		"*.{js,jsx,ts,tsx,css,md,html,json}": "npm run lint:format"
	},
	"dependencies": {
		"antd": "^5.0.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-image": "^2.1.1",
		"@rollup/plugin-node-resolve": "^13.1.2",
		"@rollup/plugin-typescript": "^8.3.0",
		"@storybook/addon-actions": "^6.4.9",
		"@storybook/addon-centered": "^5.3.21",
		"@storybook/addon-essentials": "^6.4.9",
		"@storybook/addon-info": "^5.3.21",
		"@storybook/addon-links": "^6.4.9",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/builder-webpack5": "^6.4.9",
		"@storybook/manager-webpack5": "^6.4.9",
		"@storybook/preset-scss": "^1.0.3",
		"@storybook/react": "^6.4.9",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@tsconfig/node16": "^1.0.3",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.11.17",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"autoprefixer": "^10.4.2",
		"babel-jest": "^29.3.1",
		"babel-loader": "^8.2.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-tailwind": "^0.1.10",
		"css-loader": "^6.5.1",
		"eslint": "^8.30.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-standard-with-typescript": "^24.0.0",
		"eslint-config-universe": "^11.1.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jest": "^27.1.7",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-styled-components-a11y": "^1.0.0",
		"husky": "^8.0.2",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"jest-transform-stub": "^2.0.0",
		"lint-staged": "^13.1.0",
		"postcss": "^8.4.5",
		"postcss-import": "^14.0.2",
		"prettier": "2.8.1",
		"react-icons": "^4.3.1",
		"rollup": "^2.62.0",
		"rollup-plugin-dts": "^4.1.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-scss": "^3.0.0",
		"rollup-plugin-svg-import": "^1.6.0",
		"rollup-plugin-tailwind": "^1.0.0",
		"rollup-plugin-tailwindcss": "^1.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.45.2",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"tailwindcss": "^3.0.12",
		"ts-jest": "^29.0.3",
		"tslib": "^2.3.1",
		"typescript": "^4.9.4",
		"webpack-cli": "^4.9.1"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-icons": "^4.3.1",
		"webpack": "*"
	}
}
