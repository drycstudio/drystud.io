{
  "name": "electron-titlebar",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/main.cjs",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "require": "./dist/config.cjs",
      "types": "./dist/config.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "description": "A pretty way to add Titlebar in a Electron app using ReactJS",
  "keywords": [
    "awesome",
    "pretty",
    "electron",
    "react",
    "titlebar",
    "menu",
    "plugin"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/drycstud.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://www.github.com/drycstudio/drystud.io"
  },
  "author": "euclidesdry <euclidesdry@gmail.com>",
  "packageManager": "yarn@4.3.1",
  "scripts": {
    "ci": "yarn install && yarn lint && yarn test",
    "dev": "vite",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "preview": "tsc --p ./tsconfig.build.json && vite preview",
    "build:with-storybook": "yarn build && yarn build-storybook",
    "publish:package": "yarn npm publish --access public",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "lint:format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build --docs",
    "storybook:docs": "storybook dev --docs",
    "build-storybook": "storybook build",
    "prepublishOnly": "yarn build"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "electron": "*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@stitches/core": "^1.2.8",
    "@stitches/react": "^1.2.8",
    "is-electron": "^2.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "@storybook/addon-actions": "^8.2.4",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-interactions": "^8.2.4",
    "@storybook/addon-links": "^8.2.4",
    "@storybook/addon-onboarding": "^8.2.4",
    "@storybook/blocks": "^8.2.4",
    "@storybook/react": "^8.2.4",
    "@storybook/react-vite": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@testing-library/dom": "^10.3.2",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.3",
    "c8": "^10.1.2",
    "electron": "^31.2.1",
    "eslint": "9.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-config-universe": "^13.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-styled-components-a11y": "^2.1.35",
    "globals": "^15.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.1.0",
    "prettier": "3.3.3",
    "react-icons": "^5.2.1",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.77.8",
    "storybook": "^8.2.4",
    "typescript-eslint": "^7.16.1",
    "vite": "^5.3.4",
    "vite-plugin-dts": "^4.0.0-beta.1",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.3"
  }
}
